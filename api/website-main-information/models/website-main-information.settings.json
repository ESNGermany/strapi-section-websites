{
  "kind": "collectionType",
  "collectionName": "website_main_informations",
  "info": {
    "name": "Website Main Information",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "sectionShortName": {
      "type": "string",
      "required": true
    },
    "facebookLink": {
      "type": "string",
      "required": false,
      "default": "https://www.facebook.com/"
    },
    "instagramLink": {
      "type": "string",
      "required": false,
      "default": "https://www.instagram.com/"
    },
    "pretixLink": {
      "type": "string",
      "default": "https://tickets.esn-germany.de/"
    },
    "addressNameFirstLine": {
      "type": "string",
      "required": true
    },
    "addressStreetSecondLine": {
      "type": "string",
      "required": true
    },
    "addressCityThirdLine": {
      "type": "string",
      "required": true
    },
    "addressEmailFourthLine": {
      "type": "string",
      "required": true
    },
    "welcomeMessageFrontPage": {
      "type": "string",
      "required": true
    },
    "headerImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "imageGridFrontPage": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "buttonColor": {
      "type": "enumeration",
      "enum": [
        "esnGreen",
        "esnLightBlue",
        "esnDarkBlue",
        "esnPink",
        "esnOrange"
      ],
      "required": true,
      "default": "esnGreen"
    },
    "facebookName": {
      "type": "string"
    },
    "instagramName": {
      "type": "string"
    },
    "sectionLongName": {
      "type": "string",
      "required": true
    },
    "websiteMetaDescription": {
      "type": "text",
      "required": true
    },
    "eventPageText": {
      "type": "text",
      "required": true
    },
    "usePretixCalendar": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
